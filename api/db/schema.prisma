generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  // url = "sqlserver://192.168.0.104\\TEST;instance=TEST;database=Sec2023;user=hwn83;password=nwH83;TrustServerCertificate=true;encrypt=false;"
}

model Post {
  id      Int     @id @default(autoincrement())
  title   String
  content String?
  User    User?   @relation(fields: [userId], references: [id])
  userId  Int?
}

model User {
  id    Int     @id @default(autoincrement())
  name  String
  email String?
  posts Post[]
}

model TestUser {
  id    Int     @id @default(autoincrement())
  name  String
  email String?
}

model SgmKalem {
  id         Int     @id @default(autoincrement())
  kalem_kodu String
  kalem_adi  String?
}
